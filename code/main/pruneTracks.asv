function [ playerCollection ] = pruneTracks( playerCollection, cutOffDis )
    
    newCount=0;
    for i=1:playerCollection.count
       if (playerCollection.list(i).steps<cutoffDis)
           continue;
       end
        newCount=newCount+1;
        newList(newCount)=playerCollection.list(i);
    end
    
    playerCollection.list=newList;
    playerCollection.li

end

